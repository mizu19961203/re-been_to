
<h1>ユーザー</h1>
<table class="table">
    <thead>
        <tr>
        	<th>ID</th>
            <th>ユーザー名</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
        <tr>
        	<td><%= user.id %></td>
            <td><%= link_to user.name , admin_user_path(user.id) %></td>
            <td><%= user.email %></td>
            <td>
            	<%= link_to "編集", edit_admin_user_path(user.id) %> |
                <%= link_to "削除", admin_user_path(user.id) , method: :delete ,"data-confirm" => "本当に削除しますか？"%>
            </td>
        </tr>
    <% end %>
    </tbody>
</table>