<div class="search-form">
  <%= form_tag(search_path, method: :get) do %>
    <p class="search_text"><b>ワクチン</b></p>
    <p class="search_text"><%= collection_select(:vaccine, :id, Vaccine.all, :id, :name,:include_blank => '選択してください')%></p>
    <p class="search_text"><b>国名</b></p>
    <p class="search_text"><%= collection_select(:country, :id, Country.all, :id, :country,:include_blank => '選択してください')%></p>
    <p class="search_bottun"><%= submit_tag "検索" %></p>
  <% end %>
</div>